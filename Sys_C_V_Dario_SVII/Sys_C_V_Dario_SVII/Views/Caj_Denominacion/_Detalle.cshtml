@model Sys_C_V_Dario_SVII.Models.Caj_Denominacion.Caj_DenominacionBE

<!-- Modal DETALLE-->
<div class="modal fade" id="MDetalleDenominacion" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background-color:#E17F29">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <center><h4 class="modal-title" style="color:#ffffff">DETALLE DE DENOMINACION</h4></center>
            </div>

            <div class="modal-body">

                <form class="form">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label class="control-label">ID:</label>
                            </div>
                            <div class="col-sm-8 col-md-8 col-lg-7">
                                @Html.TextBox("DtxtidDenominacion", Model.in_idDenominacion, new { type = "text", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label class="control-label">FECHA DE REGISTRO:</label>
                            </div>
                            <div class="col-sm-8 col-md-8 col-lg-7">
                                @Html.TextBox("DtxtFchRegistro", Model.dt_fchRegistro.ToString("yyyy-MM-dd"), new { type = "date", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-4 col-lg-4">
                                <label class="control-label">DESCRIPCION:</label>
                            </div>
                            <div class="col-sm-8 col-md-8 col-lg-7">
                                @Html.TextBox("DtxtDscpDenominacion", Model.st_dscpDenominacion, new { type = "text", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>
                </form>
            </div>


            <div class="modal-footer">
                <button type="button" name="btnSalir" id="btnSalir" class="btn btn-default" data-dismiss="modal">SALIR</button>

            </div>
        </div>
    </div>
</div>

