@model IEnumerable<Sys_C_V_Dario_SVII.Models.Mod_Venta.Vta_Venta.Vta_VentaBE>

<thead>
    <tr>
        <th>Venta</th>
        <th>Fecha de Registro</th>
        <th>Registrador</th>
        <th>Cliente</th>
        @*<th>Tipo Comprobante</th>*@
        <th>Operaciones</th>
        <th>Comprobante</th>
    </tr>
</thead>
@if (Model != null)
{

    foreach (var item in Model)
    {
        <tbody>
            <tr>
                <td>@Html.DisplayFor(modelItem => item.i_idVenta)</td>
                <td>@Html.DisplayFor(modelItem => item.dt_fchRegistro)</td>
                <td>@Html.DisplayFor(modelItem => item.oUsuario.oPersona.nombreCompleto)</td>
                <td>@Html.DisplayFor(modelItem => item.oPersona.nombreCompleto)</td>
                @*<td>@Html.DisplayFor(modelItem => item.oTipoEstado.vc_dscpTipEstado)</td>*@
                @*<td>@Html.DisplayFor(modelItem => item.oComprobante.oTipo_ComprobanteBE.vc_dscpTipComprobante)</td>*@
                <td>
                    <button type="button" class="btn btn-info btn-xs" name="btnDetalles" id="btnDetalles" onclick="fn_DetallesVenta('@item.i_idVenta')">Detalles</button>
                    <button type="button" class="btn btn-info btn-xs" name="btnDetalles" id="btnDetalles">Anular</button>
                </td>
                <td>
                    <button type="button" class="btn btn-info btn-xs" name="btnDetalles" id="btnDetalles" onclick="fn_GenerarVenta()">Generar</button>
                </td>
            </tr>
        </tbody>}}