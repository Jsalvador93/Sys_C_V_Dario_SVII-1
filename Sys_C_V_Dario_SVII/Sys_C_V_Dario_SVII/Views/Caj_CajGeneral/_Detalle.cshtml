@model Sys_C_V_Dario_SVII.Models.Caj_CajGeneral.Caj_CajGeneralBE

<!-- Modal -->
<div class="modal fade" id="MDetalleCajGeneral" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog ">
        <div class="modal-content">
            <div class="modal-header" style="background-color:#E17F29;">
                <button type="button" class="close" id="close" data-dismiss="modal">&times;</button>
                <h4>Detalles de la Caja General</h4>
            </div>
            <div class="modal-body">
                <form class="form">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("DtxtidCajGeneral", "ID:")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox("DtxtidCajGeneral", Model.in_idCajGeneral, new { type = "text", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("DtxtFchRegistro", "Fch Registro:")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox("DtxtFchRegistro", Model.dt_fchRegistro, new { type = "text", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("DtxtFchInicio", "Fch Inicio:")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox("DtxtFchInicio", Model.dt_fchInicio, new { type = "text", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("DtxtFchFin", "Fch Fin:")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox("DtxtFchFin", Model.dt_fchFin, new { type = "text", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("DtxtmntCajChica", "Caja Chica:")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox("DtxtmntCajChica", Model.fl_mntCajChica, new { type = "text", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("DtxtmntSdoCaja", "Saldo Caja:")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox("DtxtmntSdoCaja", Model.fl_mntSdoCaja, new { type = "text", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>

                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" name="btnSalir" id="btnSalir"> SALIR </button>
            </div>
        </div>
    </div>
</div>



