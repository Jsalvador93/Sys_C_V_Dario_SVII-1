@model IEnumerable<Sys_C_V_Dario_SVII.Models.Sys_Impuestos.Sys_ImpuestoBE>

@{
    ViewBag.Title = "Index";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>Ajuste de Impuesto</h1>
        <ol class="breadcrumb">
            <li><a href="#">Empresa</a></li>
            <li class="active">Ajuste de Impuestos</li>
        </ol>
    </section>

    <!-- Main content -->

    <section class="content">
        <div class="row">
            <div class="col-md-12">

                <div class="box box-primary">
                    <form role="form">
                        <div class="form-group">
                            <div class="box-body">
                                <div>
                                    <h4 class="col-md-3">Codigo de Impuesto : </h4>
                                    <h4 class="col-md-4">00001</h4>
                                    <h4 class="col-md-3">Fecha de Registro:</h4>
                                    <h4 class="col-md-2">21/11/2016</h4>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>

                <div class="box box-body">
                    <form role="form">
                        <div class="form-group">
                            <div class="box-body">
                                <div>
                                    <h4 class="col-md-2">Nombre: </h4>
                                    <div class="col-md-4">
                                        <input type="text" class="form-group" id="vc_dscpImpuesto">
                                    </div>
                                    <h4 class="col-md-2 text-left">Abreviatura</h4>
                                    <div class="col-md-4">
                                        <input type="text" class="form-group" id="vc_dscpAbreviatura">
                                    </div>
                                    <br />
                                    <h4 class="col-md-2">Porcentaje:</h4>
                                    <div class="col-md-4">
                                        <input type="text" class="form-group" id="vc_dscpPorcentaje">&nbsp;%
                                    </div>
                                    <div class="col-md-12">
                                        <div class="CantidadBotones">
                                            <button type="button" class="btn btn-primary">Guardar</button>
                                            <button type="button" class="btn btn-default">Cancelar</button>

                                        </div>
                                        <style>
                                            .CantidadBotones {
                                                text-align: right;
                                            }
                                        </style>

                                    </div>
                                    <script type="text/javascript">
        $('.clockpicker').clockpicker();
                                    </script>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>



    </section>
</div>

>