<!-- Trigger the modal with a button -->
<button type="button" class="btn btn-info" data-toggle="modal" data-target="#MConfirmacionAsigCajero" style="width:100%;">Asignar</button>

<!-- Modal -->
<div class="modal fade" id="MConfirmacionAsigCajero" role="dialog">
    <div class="modal-dialog modal-sm">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <p> ¿ESTA SEGURO QUE DESEA ASIGNAR ESTA CAJA AL CAJERO :?</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-info" data-dismiss="modal" name="btnSi" id="btnSi"> SI </button>
                <button type="button" class="btn btn-default" data-dismiss="modal" name="btnNo" id="btnNO"> NO </button>
            </div>
        </div>
    </div>
</div>


