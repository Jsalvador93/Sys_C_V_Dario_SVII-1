@model IEnumerable<Sys_C_V_Dario_SVII.Models.Rol_Usuario.Rol_UsuarioBE>

@{
    ViewBag.Title = "Index";
}


    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>Nuevo usuario</h1>
        <ol class="breadcrumb">
            <li><a href="#">Sistema</a></li>
            <li class="active">Nuevo Usuario</li>
        </ol>
    </section>
    <!-- Main NUEVO USUARIO -->
    <section class="content">
        <div class="row">
            <div class="col-md-12">
                <div class="box box-primary">

                    <div class="box box-primary">
                        <form role="form">
                            <div class="form-group">
                                <div class="box-body">
                                    <div class="form-group">
                                        <div class="col-md-4">
                                            <label for="Tipo_Documento">Tipo de Documento : </label>
                                        </div>
                                        <div class="col-md-4">
                                            <select class="form-control">
                                                <option></option>
                                                <option>DNI</option>
                                                <option>CARNET EXTRANJERIA</option>
                                            </select>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" id="c_codPersona">
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <!-- nombre apellidos  -->
            <div class="col-md-12">
                <div class="box box-primary">

                    <div class="box box-primary">
                        <form role="form">
                            <div class="form-group">
                                <div class="box-body">
                                </div>

                            </div>

                            <div class="form-group">
                                <div class="col-md-3">
                                    <label for="Nombres">Nombres : </label>
                                </div>
                                <div class="col-md-2">
                                    <input type="text" class="form-control" id="vc_nomPersona">
                                </div>
                                <div class="col-md-3">
                                    <label for="Ape_Pat">Apellido Paterno : </label>
                                </div>
                                <div class="col-md-2">
                                    <input type="text" class="form-control" id="vc_apPaterno">
                                </div>
                                <br />
                                <br />
                                <div class="col-md-3">
                                    <label for="Ape_Mat">Apellido Materno : </label>
                                </div>
                                <div class="col-md-2">
                                    <input type="text" class="form-control" id="vc_apMaterno">
                                </div>
                                <div class="col-md-3">
                                    <label for="Ape_Mat">Correo Electronico : </label>
                                </div>
                                <div class="col-md-2">
                                    <input type="text" class="form-control" id="vc_drcWeb">
                                </div>
                                <div class="box-body">
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- BOTONOES -->
        <div class="row">
            <div class="col-md-12">
                <div class="box box-primary">

                    <div class="box box-primary">
                        <form role="form">
                            <div class="box-body">
                                <div class="form-group">
                                    <div class="col-md-3">

                                        <div class="col-md-12">
                                            <div class="CantidadBotones">
                                                <button type="button" class="btn btn-primary">Guardar</button>
                                                <button type="button" class="btn btn-primary">Cancelar</button>
                                            </div>
                                            <style>
                                                .CantidadBotones {
                                                    text-align: right;
                                                }
                                            </style>

                                        </div>
                                        <script type="text/javascript">
                                            $('.clockpicker').clockpicker();
                                        </script>
                                    </div>
                                </div>
                                </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>

<!-- Pie de Pagina -->
