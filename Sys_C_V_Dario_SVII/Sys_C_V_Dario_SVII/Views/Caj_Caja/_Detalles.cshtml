@model Sys_C_V_Dario_SVII.Models.Caj_Caja.Caj_CajaBE
<!-- Modal -->
<div class="modal fade" id="MDetalles" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog ">
        <div class="modal-content">
            <div class="modal-header" style="background-color:#E17F29;">
                <button type="button" class="close" id="close" data-dismiss="modal">&times;</button>
                <h4>Detalles de la Caja</h4>
            </div>
            <div class="modal-body">
                <form class="form">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("txtSucursal", "Sucural:")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox("DtxtSucursal", Model.objPtoVenta.objSucursal.st_dscpSucursal , new { type = "text", disabled = "true" , @class = "form-control" })
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("txtPtoVenta", "Pto Venta:")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox("DtxtPtoVenta", Model.objPtoVenta.st_dscpPtoVenta , new { type = "text", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("txtidCaja", "ID :")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox("DtxtidCaja", Model.in_idCaja , new { type = "text", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("txtCaja", "Caja:")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox( "DtxtCaja", Model.st_dscpCaja, new { type = "text", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("txtFchIngreso", "F. Ingreso:")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox("DtxtFchIngreso", Model.d_fchIngreso.ToString("yyyy-MM-dd"), new { type = "date", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-sm-4 col-md-3 col-lg-2">
                                @Html.Label("txtFchRegistro", "F. Registro:")
                            </div>
                            <div class="col-sm-8 col-md-9 col-lg-10">
                                @Html.TextBox("DtxtFchRegistro", Model.dt_fchRegistro, new { type = "datetime", disabled = "true", @class = "form-control" })
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" name="btnSalir" id="btnSalir"> SALIR </button>
            </div>
        </div>
    </div>
</div>

